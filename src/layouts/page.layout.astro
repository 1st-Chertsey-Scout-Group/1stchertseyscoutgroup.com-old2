---
import BaseLayout from "../layouts/base.layout.astro";

import Footer from "../components/footer-navigation.component.astro";
import Header from "../components/header-navigation.component.astro";


interface Props {
  title: string;
}

const { title } = Astro.props;
---

<BaseLayout title="{title}">
  <Header />
  <main>
    <slot/>
  </main>
  <Footer />
</BaseLayout>
